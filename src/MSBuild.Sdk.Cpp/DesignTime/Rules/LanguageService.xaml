<Rule
    xmlns="http://schemas.microsoft.com/build/2009/properties"
    Name="LanguageService"
    DisplayName="LanguageService"
    PageTemplate="generic">
    <Rule.DataSource>
        <DataSource Persistence="ProjectFile" SourceOfDefaultValue="AfterContext" />
    </Rule.DataSource>

    <StringProperty
        Name="RootNamespace"
        ReadOnly="True"
        Visible="False" />

    <StringProperty
        Name="TargetPath"
        ReadOnly="True"
        Visible="False" />

</Rule>
