<Rule xmlns="http://schemas.microsoft.com/build/2009/properties" Name="UpToDateCheckOutput">
    <Rule.DataSource>
        <DataSource
            HasConfigurationCondition="False"
            MSBuildTarget="CollectUpToDateCheckOutputDesignTime"
            Persistence="ProjectFile"
            SourceOfDefaultValue="AfterContext"
            SourceType="TargetResults" />
    </Rule.DataSource>

    <BoolProperty
        Name="Visible"
        ReadOnly="True"
        Visible="False" />

</Rule>
